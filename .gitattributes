# Audio files
*.wav filter=lfs diff=lfs merge=lfs -text
*.mp3 filter=lfs diff=lfs merge=lfs -text
*.ogg filter=lfs diff=lfs merge=lfs -text
*.flac filter=lfs diff=lfs merge=lfs -text

# Model files
*.bin filter=lfs diff=lfs merge=lfs -text
*.model filter=lfs diff=lfs merge=lfs -text
*.hmm filter=lfs diff=lfs merge=lfs -text
*.lm filter=lfs diff=lfs merge=lfs -text

# Large data files
*.dat filter=lfs diff=lfs merge=lfs -text
*.csv filter=lfs diff=lfs merge=lfs -text
*.json filter=lfs diff=lfs merge=lfs -text

# Test audio files
tests/fixtures/*.wav filter=lfs diff=lfs merge=lfs -text
tests/fixtures/*.mp3 filter=lfs diff=lfs merge=lfs -text
tests/fixtures/*.ogg filter=lfs diff=lfs merge=lfs -text

# Documentation
*.pdf filter=lfs diff=lfs merge=lfs -text
*.doc filter=lfs diff=lfs merge=lfs -text
*.docx filter=lfs diff=lfs merge=lfs -text

# Source code
*.cpp text
*.h text
*.hpp text
*.ts text
*.js text
*.json text
*.md text
*.txt text
*.yml text
*.yaml text
*.sh text
*.bat text
*.cmd text 